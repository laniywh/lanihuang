{
  "name": "eleventy-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:tailwind": "tailwindcss -i ./src/assets/css/input.css -o ./src/assets/css/output.css --watch --postcss",
    "dev:eleventy": "cross-env ELEVENTY_ENV=DEV eleventy --serve",
    "build:css": "tailwindcss -i src/assets/css/input.css -o ./src/assets/css/output.css --postcss",
    "build:eleventy": "cross-env ELEVENTY_ENV=PROD eleventy",
    "dev": "npm-run-all --parallel dev:*",
    "build": "run-s build:*"
  },
  "keywords": [],
  "author": "Lani Huang",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.48",
    "tailwindcss": "^3.4.14"
  },
  "packageManager": "pnpm@9.12.0"
}
